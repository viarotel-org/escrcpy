{
  "name": "@escrcpy/workspace",
  "type": "module",
  "version": "2.0.1",
  "private": true,
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa",
  "description": "Scrcpy Powered by Electron",
  "author": "viarotel",
  "homepage": "https://github.com/viarotel-org/escrcpy",
  "main": "dist-electron/main.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "turbo dev",
    "build": "turbo build:electron",
    "build:win": "turbo run build:electron -- --win",
    "build:mac": "turbo run build:electron -- --mac",
    "build:linux": "turbo run build:electron -- --linux",
    "svgo": "pnpm --filter @escrcpy/desktop svgo",
    "electron-fix": "pnpm --filter @escrcpy/desktop electron-fix",
    "lang-sync": "node ./scripts/lang-sync.js",
    "clean": "pnpm dlx rimraf --glob **/node_modules **/dist* **/.turbo",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.16.2",
    "eslint": "9.31.0",
    "husky": "9.0.11",
    "tsdown": "0.17.3",
    "turbo": "2.7.2",
    "vitepress": "1.6.4",
    "vitepress-i18n": "1.3.3",
    "vitepress-sidebar": "1.31.1"
  }
}
