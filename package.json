    "@electron-toolkit/utils": "3.0.0",
    "@electron/remote": "2.1.2",
    "@intlify/unplugin-vue-i18n": "4.0.0",
    "@unocss/reset": "0.62.3",
    "@unocss/transformer-directives": "0.62.3",
